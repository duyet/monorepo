{
  "name": "insights",
  "version": "0.1.0",
  "private": true,
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "scripts": {
    "config": "bun ../../scripts/sync-app-secrets.ts duyet-insights",
    "build": "bun --bun next build",
    "check-types": "tsc --noEmit",
    "dev": "bun --bun next dev --turbopack -p 3001",
    "fmt": "bun run fix",
    "lint": "biome lint .",
    "prettier-check": "prettier --check .",
    "start": "bun --bun next start",
    "deploy": "bun run build && wrangler pages deploy out --project-name=duyet-insights",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@clickhouse/client": "^1.12.1",
    "@duyet/components": "*",
    "@duyet/config": "*",
    "@duyet/libs": "*",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-tabs": "^1.1.12",
    "clsx": "^2.1.1",
    "encoding": "^0.1.13",
    "graphql": "^16.6.0",
    "graphql-request": "^6.0.0",
    "lucide-react": "^0.562.0",
    "nanoid": "^5.0.0",
    "next": "16.1.1",
    "next-themes": "^0.4.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-is": "^19.1.0",
    "recharts": "^3.0.2",
    "swr": "^2.0.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@duyet/interfaces": "*",
    "@duyet/prettier": "*",
    "@duyet/tailwind-config": "*",
    "@duyet/tsconfig": "*",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^24.0.0",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "autoprefixer": "^10.4.12",
    "postcss": "^8.4.18",
    "tailwindcss": "*",
    "typescript": "*",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.1.0",
    "@testing-library/dom": "^10.4.0",
    "@types/jest": "^30.0.0",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0"
  }
}
