{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "NEXT_PUBLIC_DUYET_CV_URL",
    "NEXT_PUBLIC_DUYET_BLOG_URL",
    "NEXT_PUBLIC_DUYET_INSIGHTS_URL",
    "KV_REST_API_READ_ONLY_TOKEN",
    "KV_REST_API_TOKEN",
    "KV_REST_API_URL",
    "KV_URL",
    "NEXT_PUBLIC_AUTH0_ADMIN_EMAIL",
    "NEXT_PUBLIC_AUTH0_CLIENT_ID",
    "NEXT_PUBLIC_AUTH0_DOMAIN",
    "NEXT_PUBLIC_CLOUDFLARE_API_KEY",
    "NEXT_PUBLIC_CLOUDFLARE_API_TOKEN",
    "NEXT_PUBLIC_CLOUDFLARE_ZONE_ID",
    "NEXT_PUBLIC_MEASUREMENT_ID",
    "NEXT_PUBLIC_AXIOM_DATASET",
    "NEXT_PUBLIC_AXIOM_TOKEN",
    "NEXT_PUBLIC_SELINE_TOKEN",
    "POSTGRES_DATABASE",
    "POSTGRES_HOST",
    "POSTGRES_PASSWORD",
    "POSTGRES_PRISMA_URL",
    "POSTGRES_URL",
    "POSTGRES_URL_NON_POOLING",
    "POSTGRES_USER",
    "POSTHOG_API_KEY",
    "POSTHOG_PROJECT_ID",
    "GITHUB_TOKEN"
  ],
  "globalDependencies": [".env", ".env.local"],
  "tasks": {
    "build": {
      "outputs": [".next/**", "!.next/cache/**", "dist/**"],
      "dependsOn": ["^build"]
    },
    "lint": {},
    "fmt": {},
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "test:watch": {
      "cache": false,
      "persistent": true
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "deploy": {
      "dependsOn": ["build", "test", "lint"]
    },
    "clean": {
      "cache": false
    }
  }
}
