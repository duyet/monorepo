{
  "name": "ai-percentage",
  "version": "0.1.0",
  "private": true,
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "scripts": {
    "config": "bun ../../scripts/sync-app-secrets.ts duyet-ai-percentage",
    "build": "bun next build",
    "check-types": "tsc --noEmit",
    "dev": "bun --bun next dev --turbopack -p 3002",
    "fmt": "bun run fix",
    "lint": "biome lint .",
    "start": "bun --bun next start",
    "deploy": "bun run build && bunx wrangler pages deploy out --project-name=duyet-ai-percentage"
  },
  "dependencies": {
    "@duyet/config": "*",
    "@duyet/libs": "*",
    "lucide-react": "^0.563.0",
    "next": "16.1.5",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "recharts": "^3.0.2"
  },
  "devDependencies": {
    "@duyet/tailwind-config": "*",
    "@duyet/tsconfig": "*",
    "@types/react": "^19.1.4",
    "typescript": "*"
  }
}
