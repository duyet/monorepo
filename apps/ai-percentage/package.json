{
  "name": "ai-percentage",
  "version": "0.1.0",
  "private": true,
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "scripts": {
    "config": "bun ../../scripts/sync-app-secrets.ts duyet-ai-percentage",
    "build": "bun next build",
    "check-types": "tsc --noEmit",
    "dev": "bun --bun next dev --turbopack -p 3002",
    "fmt": "bun run fix",
    "lint": "biome lint .",
    "prettier-check": "prettier --check .",
    "start": "bun --bun next start",
    "deploy": "bun run build && bunx wrangler pages deploy out --project-name=duyet-ai-percentage"
  },
  "dependencies": {
    "@duyet/config": "*",
    "@duyet/libs": "*",
    "@radix-ui/react-icons": "^1.3.0",
    "@tailwindcss/postcss": "^4.1.18",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "next-themes": "^0.4.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "recharts": "^3.0.2",
    "swr": "^2.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@duyet/prettier": "*",
    "@duyet/tailwind-config": "*",
    "@duyet/tsconfig": "*",
    "@types/node": "^25.0.0",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "autoprefixer": "^10.4.12",
    "postcss": "^8.4.18",
    "tailwindcss": "*",
    "typescript": "*"
  }
}
